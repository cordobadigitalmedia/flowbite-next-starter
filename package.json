{
  "type": "module",
  "name": "lms-nextjs",
  "version": "2.0.0",
  "description": "LMS template using Flowbite React on Next.js 14 with clerk",
  "homepage": "https://github.com/cordobadigitalmedia/lms-nextjs",
  "bugs": "https://github.com/cordobadigitalmedia/lms-nextjs/issues",
  "license": "MIT",
  "author": "Ismael Rumzan <ismael@cordobadigitalmedia.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/cordobadigitalmedia/lms-nextjs.git"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
